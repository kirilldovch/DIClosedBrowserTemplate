<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Здесь цвета даны для примера, замените на те, которые есть в макете-->
    <Color x:Key="BlackColor">#292929</Color>
    <Color x:Key="YellowColor">#FFBE02</Color>
    <Color x:Key="OrangeColor">#FE6A02</Color>
    <Color x:Key="AlmostWhite">#ECF5EF</Color>

    <SolidColorBrush x:Key="TextBrush" Color="{StaticResource BlackColor}" />
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource YellowColor}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource OrangeColor}" />
    <SolidColorBrush x:Key="ThirdBrush" Color="{StaticResource AlmostWhite}" />


    <!--Цвет подложки клавиатуры-->
    <Brush x:Key="KeyboardBackground">#F6F5F5</Brush>
    <!--Цвет шрифта кнопок-->
    <Brush x:Key="KeyboardButtonForeground">#071343</Brush>
    <!--Цвет кнопок клавиатуры-->
    <Brush x:Key="KeyboardButtonBackground">#FFFFFF</Brush>
    <!--Цвет подложки кнопок клавиатуры-->
    <Brush x:Key="KeyboardHighlightedBackground">#EBF8FF</Brush>

    <!--Цвет кнопок-->
    <Brush x:Key="ButtonBackground">#35BDC8</Brush>
    <!--Цвет подложки кнопок-->
    <Brush x:Key="ButtonPanelBackground">#29333f</Brush>
    <!--Цвет иконок-->
    <Brush x:Key="IconBackground">#FFFFFF</Brush>
</ResourceDictionary>